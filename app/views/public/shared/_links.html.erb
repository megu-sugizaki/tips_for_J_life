<%- if controller_name != 'sessions' %>
  <u><%= link_to "Sign in", new_session_path(resource_name), class:"text-dark" %></u>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <u><%= link_to "Sign up", new_registration_path(resource_name), class:"text-dark" %></u>
<% end %>
